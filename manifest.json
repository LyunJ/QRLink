{
    "name":"Where are my tabs",
    "description":"Sending Chrome tabs between pc and mobile phone",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"firebase.js"
    },
    "content_scripts":[
        {
            "matches" : ["<all_urls>"],
            "js":["app.js"],
            "run_at":"document_end"
        }
        

    ],
    "permissions":[
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions":[
        "<all_urls>"
    ],
    "action":{
        "default_popup":"popup.html"
    }
}