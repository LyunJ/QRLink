{
    "name":"Where are my tabs",
    "description":"Sending Chrome tabs between pc and mobile phone",
    "version":"1.0",
    "manifest_version":3,
    "background":{
        "service_worker":"firebase.js"
    },
    "permissions":[
        "activeTab",
        "tabs",
        "storage",
        "scripting",
        "identity"
    ],
    "host_permissions":[
        "<all_urls>"
    ],
    "action":{
        "default_popup":"popup.html"
    },
    "oauth2":{
        "client_id":"797775261594-ps4f2aks5crsbighonej54g26ekkunpk.apps.googleusercontent.com",
        "scopes":["openid", "email", "profile"]
    }
}